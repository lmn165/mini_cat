<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.cat.mappers.GalleryMapper">

	<select id="getList" resultType="Ecotype">
		select * from tb_ecotype
	</select>

	<select id="read" resultType="Ecotype">
		select * from tb_ecotype where eno = #{eno}
	</select>
</mapper>